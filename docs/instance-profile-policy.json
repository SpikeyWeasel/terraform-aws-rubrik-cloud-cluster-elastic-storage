{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Action": [
				"s3:AbortMultipartUpload",
				"s3:DeleteObject*",
				"s3:GetObject*",
				"s3:ListMultipartUploadParts",
				"s3:PutObject*"
			],
			"Effect": "Allow",
			"Resource": [
        "arn:aws:s3:::<BUCKET_NAME>/*"
      ]  
		},
		{
			"Action": [
				"s3:GetBucket*",
				"s3:ListBucket*"
			],
			"Effect": "Allow",
			"Resource": [
        "arn:aws:s3:::<BUCKET_NAME>"
      ]  
		}
	]
}
